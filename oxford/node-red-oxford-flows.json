        "format": "<html>\n<head>\n\t<script>\n\n\n(function(scope) {\n    // watch msg object from Node-RED\n    scope.$watch('msg', function(msg) {\n        if (msg.topic === \"metronome/tempo/increase\" && msg.payload === \"x\")\n        {\n            //alert('tempo UP');\n            tempo_up();\n            //msg.payload = \"\";\n        }\n        if (msg.topic === \"metronome/tempo/decrease\" && msg.payload === \"x\")\n        {\n            tempo_down();\n            //msg.payload = \"\";\n        }\n        if (msg.topic === \"song/tempo/value\")\n        {\n            $tempo.text(msg.payload);\n            tempo = parseInt($tempo.text());\n        }\n        if (msg.topic === \"metronome/activate\")\n        {\n            toggle_play();\n        }\n        //console.log('yadda');\n        // new message **msg** received\n        //alert(msg.topic); //tempo.String());\n    });\n})(scope);\n\nvar $tempo;\nvar tempo;\nvar play;\nvar snd;\n\nfunction tempo_up()\n{\n    tempo = parseInt($tempo.text());\n  \t$tempo.text(parseInt($tempo.text()) + 1);\n}\n\nfunction tempo_down()\n{\n    tempo = parseInt($tempo.text());\n    $tempo.text(parseInt($tempo.text()) - 1);\n\n}\n\n\nfunction toggle_play()\n{\n\tplay = !play;\n\tif (play) startMetronome();\n}\n\n\n\nfunction startMetronome() {\n\t\t\tfunction getInterval() {\n\t\t\t    tempo = parseInt($tempo.text());\n\t\t\t    if (tempo > 240)\n\t\t\t    {\n\t\t\t        tempo = 240;\n\t\t\t    }\n\t\t\t    if (tempo < 30)\n\t\t\t    {\n\t\t\t        tempo = 30;\n\t\t\t    }\n\t\t\t    $tempo.text(tempo);\n\t\t\t\treturn 1000 / (parseInt($tempo.text()) / 60);\n\t\t\t}\n\t\t\tvar interval = getInterval();\n\t\t\tvar started = Date.now();\n\t\t\tvar bg_toggle_bool = 0;\n            var bpm_id = document.getElementById(\"bpm\");\n\t\t\tsnd.play();\n\t\t\tsetTimeout(step, interval);\n\t\t\tfunction step() {\n\t\t\t\tif (play) {\n\t\t\t\t\tsnd.play();\n\t\t\t\t\tvar now = Date.now();\n\t\t\t\t\tvar diff = now - started - interval;\n\t\t\t\t\tinterval = getInterval();\n\t\t\t\t\tstarted = now;\n\t\t\t\t\tsetTimeout(step, interval - diff);\n\t\t\t\t\tif (bg_toggle_bool === 1)\n\t\t\t\t\t{\n\t\t\t\t\t    bg_toggle_bool = 0;\n\t\t\t\t\t    document.body.style.background = \"red\";\n\t\t\t\t\t    bpm_id.style.background = \"red\";\n\t\t\t\t\t    \n\t\t\t\t\t}\n\t\t\t\t\telse\n\t\t\t\t\t{\n\t\t\t\t\t    bg_toggle_bool = 1;\n\t\t\t\t\t    document.body.style.background = \"white\";\n\t\t\t\t\t    bpm_id.style.background = \"white\";\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\n\t$(document).ready(function() {\n\n\n\n        $tempo = $('#tempo');\n        tempo = 130;\n        \n \n\n\n\n        \n\t\tsnd = new Audio('data:audio/wav;base64,');\n\t\tplay = false;\n\t\t$('#playing').hide();\n\n\t\t$(document).on('keydown', function(e) {\n\t\t\tswitch (e.which) {\n\t\t\t//case 38: // up\n\t\t\t//\ttempo_up();\n\t\t\t//\tbreak;\n\t\t\t//case 40: // down\n            //    tempo_down();\n\t\t\t//\tbreak;\n\t\t\tcase 32: // space\n\t\t\ttoggle_play()\n\t\t\t\n\t\t\t}\n\t\t});\n\t});\n\t\n\t\n\t\n\t\n\t</script>\n\t<style>\n\t\tbody { font-family:Arial; color:#333; }\n\t\t#bpm { font-size:30pt; text-align:center; margin-top:10px; margin-bottom:10px; }\n\t\t#tempo { font-size:65pt; }\n\t</style>\n</head>\n<body>\n\t<div id=\"bpm\"><span id=\"tempo\">120</span> BPM</div>\n</body>\n</html>",
